@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <link href="@Url.Content("~/Content/content.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/attrForm.css")" rel="stylesheet" type="text/css" />
    <title>System error occured</title>
</head>
<body>
    <div style="position:absolute; top:100px; left:10px; right:10px; bottom:120px; font-size:large; text-align:center;">
        <img src="@Url.Content("~/assets/images/error/err_48_4.gif")" /> Error Occured on the system. <br />
        Please contact Admin !!!
    </div>
    <div style="position:absolute; bottom:120px; right:10px; font-size:small;">On : @DateTime.Now</div>
    <div style="position:absolute; bottom:50px; right:10px;">
        <input type="button" class="btnBlue" value="OK" id="btnAlert" onclick="window.close()" />
    </div>
</body>
</html>
