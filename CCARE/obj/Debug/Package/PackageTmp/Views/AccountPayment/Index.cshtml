@{
    Layout = "~/Views/Shared/_LayoutInquiry.cshtml";
}

@section title
{
    
}

@section mainmenu{ Search }
@section submenu{ Knowledge Base }
@section menu{ Company Partner }

<div class="col-lg-9 inquiry_left">
    <div class="input-group">
        <span class="input-group-btn inquiry_dropdown">
            <select id="inquiry_param" class="form-control">
                <option value="searchPembayaran">Jenis Pembayaran / Pembelian</option>
                <option value="searchName">Nama Perusahaan</option>
                <option value="searchKode">Kode Perusahaan</option>
                <option value="searchNoRek">No Rek Perusahaan</option>
                <option value="searchCabang">Cabang Koordinator</option>
                <option value="searchKerjasama">Jenis Kerjasama</option>
                <option value="searchAlur">Alur Transaksi ATM</option>
                <option value="searchEBanking">e-Banking</option>
                <option value="searchSandi">Sandi Perusahaan MBCA</option>
                <option value="searchLimit">Limit</option>
                <option value="searchDenominasi">Denominasi Voucher</option>
            </select>
        </span>
        <span class="input-group-btn input_space"></span>
        <input type="text" class="form-control" id="inquiry_val">
        <span class="input-group-btn">
            <button id="btn_search" class="btn btn-primary" type="submit"><span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
        </span>
    </div>
    </div>
    <div class="col-lg-3 inquiry_right">
    <div class="input-group col-lg-12">
        <span class="inquiry_dropdown">
            <select id="inquiry_filter" class="form-control">
                <option value="1">Active Company Partner</option>
                <option value="2">Inactive Company Partner</option>
            </select>
        </span>
    </div>
</div>


