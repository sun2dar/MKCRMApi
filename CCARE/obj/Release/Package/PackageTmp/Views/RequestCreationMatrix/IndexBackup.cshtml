@{
    Layout = "~/Views/Shared/_LayoutInquiry.cshtml";
}

@section title
{
    Request Creation Matrix
}


    <div class="col-lg-9 inquiry_left">
    <div class="input-group">
        <span class="input-group-btn inquiry_dropdown">
            <select id="inquiry_param" class="form-control">
                <option value="1">@Resources.InquiryParameter.RCM_parameter1</option>
                <option value="2">@Resources.InquiryParameter.RCM_parameter2</option>
                <option value="3">@Resources.InquiryParameter.RCM_parameter3</option>
                <option value="4">@Resources.InquiryParameter.RCM_parameter4</option>
                <option value="5">@Resources.InquiryParameter.RCM_parameter5</option>
                <option value="6">@Resources.InquiryParameter.RCM_parameter6</option>
                <option value="7">@Resources.InquiryParameter.RCM_parameter7</option>
            </select>
        </span>
        <span class="input-group-btn input_space"></span>
        <input type="text" class="form-control" id="inquiry_val">
        <span class="input-group-btn">
            <button id="btn_search" class="btn btn-primary" type="submit"><span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
        </span>
    </div>
    </div>
    <div class="col-lg-3 inquiry_right">
        <div class="input-group col-lg-12">
            <span class="inquiry_dropdown">
                <select id="inquiry_filter" class="form-control">
                    <option value="1">Active Request Creation Matrix</option>
                    <option value="2">Inactive Request Creation Matrix</option>
                </select>
            </span>
        </div>
    </div>


