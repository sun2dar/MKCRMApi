<script type="text/javascript">
    var TypeState = "Product";
    var ProductState = "ATM Card";
    var ParameterState = "ATM Card No";

    $(document).ready(function () {
        var gridId = "#gridTable";
        var entityName = '@ViewContext.RouteData.Values["Controller"].ToString()';
        $(".form-horizontal").find("select").addClass("rm-padding");
        $("#btn_search_inquiry").click(function () {
            (event.preventDefault) ? event.preventDefault() : event.returnValue = false;

            var url = "";
            var postData = "";

            switch (TypeState) {
                // ====================================================================================================
                // ====================================================================================================
                // ====================================================================================================
                case "Default":
                    return false;
                    break;
                case "Product":
                    switch (ProductState) {
                        case "ATM Card":
                            url = "/Deposit/DepositCardNoPartial/";
                            postData = {
                                cardNo: $("#inquiry-product-atm-val").val()
                            };
                            break;
                        case "Credit Card":
                            url = "/CreditCard/CreditCardInfoPartial/";
                            postData = {
                                CreditCardNumber: $("#inquiry-product-creditcard-val").val()
                            };
                            break;
                        case "Account":
                            url = "/Deposit/DepositInfoPartial/";
                            postData = {
                                accountNo: $("#inquiry-product-account-val").val()
                            };
                            break;
                        case "Loan":
                            url = "/Loan/LoanPartial/";
                            postData = {
                                loanNumber: $("#inquiry-product-loan-val").val(),
                                accountNo: $("#inquiry-product-loan-val-2").val()
                            };
                            break;
                    }
                    break;
                // ====================================================================================================
                // ====================================================================================================
                // ====================================================================================================
                case "eBanking":
                    switch (ProductState) {
                        case "KlikBCA Individu":
                            url = "/KlikBCAIndividu/KBIKlikBCAIndividuPartial/";
                            switch (ParameterState) {
                                case "No Kartu":
                                    postData = {
                                        cardNo: $("#inquiry-channel-ib-val").val()
                                    };
                                    break;
                                case "User ID":
                                    postData = {
                                        userId: $("#inquiry-channel-ib-val").val(),
                                    };
                                    break;
                                case "Email Address":
                                    postData = {
                                        emailAddress: $("#inquiry-channel-ib-val").val()
                                    };
                                    break;
                            }
                            break;
                        case "Koneksi Kartu Kredit IB":
                            url = "/KlikBCAIndividu/KBICCConnectionPartial/";
                            switch (ParameterState) {
                                case "Key ID Yang Meregister":
                                    postData = {
                                        userId: $("#inquiry-channel-connectib-val").val()
                                    };
                                    break;
                                case "No Customer":
                                    postData = {
                                        customerNo: $("#inquiry-channel-connectib-val").val(),
                                    };

                                    break;
                            }
                            break;
                        case "Status Permohonan Kartu Kredit IB":
                            url = "/KlikBCAIndividu/KBICCApplicationPartial/";
                            switch (ParameterState) {
                                case "Key ID Yang Meregister":
                                    postData = {
                                        userId: $("#inquiry-channel-requestib-val").val()
                                    };
                                    break;
                                case "No Customer":
                                    postData = {
                                        customerNo: $("#inquiry-channel-requestib-val").val(),
                                    };
                                    break;
                            }
                            break;
                        case "MBCA":
                            url = "/MBCA/MBCAPartial/";
                            switch (ParameterState) {
                                case "No Kartu":
                                    postData = {
                                        CardNo: $("#inquiry-channel-mbca-val").val()
                                    };
                                    break;
                                case "Handphone No":
                                    postData = {
                                        HpNo: $("#inquiry-channel-mbca-val").val(),
                                    };
                                    break;
                            }
                            break;
                        case "SMS Top Up":
                            url = "/SMSTopUp/SMSTopUpPartial/";
                            switch (ParameterState) {
                                case "No Kartu":
                                    postData = {
                                        CardNo: $("#inquiry-channel-smstopup-val").val()
                                    };
                                    break;
                                case "Handphone No":
                                    postData = {
                                        HpNo: $("#inquiry-channel-smstopup-val").val(),
                                    };
                                    break;
                            }
                            break;
                        case "BCA By Phone":
                            url = "/BCAbyPhone/BCAbyPhonePartial";
                            switch (ParameterState) {
                                case "No Kartu":
                                    postData = {
                                        CardNo: $("#inquiry-channel-bbp-val").val()
                                    };
                                    break;
                                case "No Rekening":
                                    postData = {
                                        AccountNo: $("#inquiry-channel-bbp-val").val(),
                                    };
                                    break;
                            }
                            break;
                        case "KlikBCA Bisnis":
                            url = "/KlikBCABisnis/KBBKlikBCABisnisPartial";
                            switch (ParameterState) {
                                case "Account No":
                                    postData = {
                                        AccountNo: $("#inquiry-channel-kbb-val").val()
                                    };
                                    break;
                                case "Corp ID":
                                    postData = {
                                        CorpId: $("#inquiry-channel-kbb-val").val(),
                                        userId: $("#inquiry-channel-kbb-val-2").val()
                                    };
                                    break;
                                case "Email Address":
                                    postData = {
                                        EmailAddress: $("#inquiry-channel-kbb-val").val(),
                                    };
                                    break;
                            }
                            break;

                            break;
                        case "SMS BCA":
                            url = "/SMSBCA/SMSBCAPartial/";
                            switch (ParameterState) {
                                case "No Kartu":
                                    postData = {
                                        CardNo: $("#inquiry-channel-smsbca-val").val()
                                    };
                                    break;
                                case "Handphone No":
                                    postData = {
                                        HpNo: $("#inquiry-channel-smsbca-val").val(),
                                    };
                                    break;
                            }
                            break;
                    }
                    break;
                // ====================================================================================================
                // ====================================================================================================
                // ====================================================================================================
                case "Key BCA":
                    switch (ProductState) {
                        case "Koneksi KeyBCA Individu":
                            url = "/KlikBCAIndividu/KBIKeyBCAConnectionPartial/";
                            switch (ParameterState) {
                                case "SN Token":
                                    postData = {
                                        SNKeyBCA: $("#inquiry-keybca-connectib-val").val()
                                    };
                                    break;
                                case "User ID": 
                                    postData = {
                                        UserId: $("#inquiry-keybca-connectib-val").val(),
                                    };
                                    break;
                                case "No Kartu":
                                    postData = {
                                        CardNo: $("#inquiry-keybca-connectib-val").val()
                                    };
                                    break;
                            }
                            break;
                        case "Status Permohonan KeyBCA Individu":
                            url = "/KlikBCAIndividu/KBIKeyBCAApplicationPartial/";
                            switch (ParameterState) {
                                case "SN Token": 
                                    postData = {
                                        SNKeyBCA: $("#inquiry-keybca-requestib-val").val()
                                    };
                                    break;
                                case "User ID":
                                    postData = {
                                        UserId: $("#inquiry-keybca-requestib-val").val()
                                    };
                                    break;
                                case "No Kartu":
                                    postData = {
                                        CardNo: $("#inquiry-keybca-requestib-val").val()
                                    };
                                    break;
                            }
                            break;
                        case "Koneksi KeyBCA Bisnis":
                            url = "/KlikBCABisnis/KBBKeyBCAConnectionPartial/";
                            switch (ParameterState) {
                                case "SN Token":
                                    postData = {
                                        SNKeyBCA: $("#inquiry-keybca-connectkbb-val").val()
                                    };
                                    break;
                                case "Key ID":
                                    postData = {
                                        CorpId: $("#inquiry-keybca-connectkbb-val").val(),
                                        UserId: $("#inquiry-keybca-connectkbb-val-2").val()
                                    };
                                    break;
                                case "Corp ID":
                                    postData = {
                                        CorpId: $("#inquiry-keybca-connectkbb-val").val()
                                    };
                                    break;
                            }
                            break;
                        case "Status Permohonan KeyBCA Bisnis":
                            url = "/KlikBCABisnis/KBBKeyBCAApplicationPartial/";
                            switch (ParameterState) {
                                case "SN Token":
                                    postData = {
                                        SNKeyBCA: $("#inquiry-keybca-requestkbb-val").val()
                                    };
                                    break;
                                case "Key ID":
                                    postData = {
                                        CorpId: $("#inquiry-keybca-requestkbb-val").val(),
                                        UserId: $("#inquiry-keybca-requestkbb-val-2").val()
                                    };
                                    break;
                                case "Corp ID":
                                    postData = {
                                        CorpId: $("#inquiry-keybca-requestkbb-val").val()
                                    };
                                    break;
                            }
                            break;
                    }
                    break;
            }

            var $btn = $(this);

            $.ajax({
                type: "GET",
                url: url,
                data: postData,
                cache: false,
                contentType: "application/html; charset=utf-8",
                dataType: "html",
                beforeSend: function () {
                    $btn.button('loading');
                },
                success: function (viewHTML) {
                    $("#partialView").html(viewHTML);
                    //$(".form-horizontal").find("input").prop("disabled", false);
                },
                error: function (errorData) {
                    //onError(errorData);
                },
                complete: function () {
                    $btn.button('reset');
                    //setTimeout(function () {
                    //    $btn.button('reset');
                    //}, 1000);
                }
            });
        });

        // ====================================================================================================
        // ====================================================================================================
        $("#inquiry-type").change(function(){
            HideAll();
            var v = $(this).val();
            if(v == 'default'){
                TypeState = "Default";
                ProductState = "";
                ParameterState = "";
                HideAll();
            }
            else if(v == 'product'){
                TypeState = "Product";
                ProductState = "ATM Card";
                $("#inquiry-product").show();
                $("#inquiry-product").val('atmcard');
                $("#inquiry-product-atm").show();
            }
            else if (v == 'ebanking') {
                TypeState = "eBanking";
                ProductState = "KlikBCA Individu";
                ParameterState = "No Kartu";
                $("#inquiry-channel").show();
                $("#inquiry-channel").val('ib');
                $("#inquiry-channel-ib").show();
            }
            else if(v == 'keybca'){
                TypeState = "Key BCA";
                ProductState = "Koneksi KeyBCA Individu";
                ParameterState = "SN Token";
                $("#inquiry-keybca").show();
                $("#inquiry-keybca").val('keybcaconnectib');
                $("#inquiry-keybca-connectib").show();
            }

            if (TypeState == 'Default') {
                $("#inqType").html('');
            }
            else {
                $("#inqType").html(TypeState);
            }
            clsTxt();
        });
        // ====================================================================================================
        // ====================================================================================================
        $("#inquiry-product").change(function(){
            HideProduct();
            var v = $(this).val();
            switch(v)
            {
                case 'atmcard':
                    ProductState = "ATM Card";
                    $("#inquiry-product-atm").show();
                    break;
                case 'creditcard':
                    ProductState = "Credit Card";
                    $("#inquiry-product-creditcard").show();
                    break;
                case 'account':
                    ProductState = "Account";
                    $("#inquiry-product-account").show();
                    break;
                case 'loan':
                    ProductState = "Loan";
                    $("#inquiry-product-loan").show();
                    break;
            }
            clsTxt();
        });

        $("#inquiry-channel").change(function () {
            HideChannel();
            var v = $(this).val();
            switch(v)
            {
                case 'ib':
                    ProductState = "KlikBCA Individu";
                    ParameterState = "No Kartu";
                    $("#inquiry-channel-ib").show();
                    break;
                case 'connectib':
                    ProductState = "Koneksi Kartu Kredit IB";
                    ParameterState = "Key ID Yang Meregister";
                    $("#inquiry-channel-connectib").show();
                    break;
                case 'requestib':
                    ProductState = "Status Permohonan Kartu Kredit IB";
                    ParameterState = "Key ID Yang Meregister";
                    $("#inquiry-channel-requestib").show();
                    break;
                case 'mbca':
                    ProductState = "MBCA";
                    ParameterState = "No Kartu";
                    $("#inquiry-channel-mbca").show();
                    break;
                case 'smstopup':
                    ProductState = "SMS Top Up";
                    ParameterState = "No Kartu";
                    $("#inquiry-channel-smstopup").show();
                    break;
                case 'bbp':
                    ProductState = "BCA By Phone";
                    ParameterState = "No Kartu";
                    $("#inquiry-channel-bbp").show();
                    break;
                case 'kbb':
                    ProductState = "KlikBCA Bisnis";
                    ParameterState = "Account No";
                    $("#inquiry-channel-kbb").show();
                    break;
                case 'smsbca':
                    ProductState = "SMS BCA";
                    ParameterState = "No Kartu";
                    $("#inquiry-channel-smsbca").show();
                    break;
            }
            clsTxt();
        });

        $("#inquiry-keybca").change(function(){
            HideKeyBCA();
            var v = $(this).val();
            switch(v)
            {
                case 'keybcaconnectib':
                    ProductState = "Koneksi KeyBCA Individu";
                    ParameterState = "SN Token";
                    $("#inquiry-keybca-connectib").show();
                    break;
                case 'keybcarequestib':
                    ProductState = "Status Permohonan KeyBCA Individu";
                    ParameterState = "SN Token";
                    $("#inquiry-keybca-requestib").show();
                    break;
                case 'keybcaconnectkbb':
                    ProductState = "Koneksi KeyBCA Bisnis";
                    ParameterState = "SN Token";
                    $("#inquiry-keybca-connectkbb").show();
                    break;
                case 'keybcarequestkbb':
                    ProductState = "Status Permohonan KeyBCA Bisnis";
                    ParameterState = "SN Token";
                    $("#inquiry-keybca-requestkbb").show();
                    break;
            }
            clsTxt();
        });
        // ====================================================================================================
        // ====================================================================================================
        $("#btn_clear").click(function () {
            $("#partialView").html('');
            clsTxt();
        });

        function clsTxt() {
            $("input[type=text]").val('');
            $(".form-horizontal").find("input").prop("disabled", false);
        }
        
        function Init() {
            HideAll();
            $("#inquiry-type").trigger('change');
        }
        Init();
        // ====================================================================================================
        // ====================================================================================================
        function HideAll() {
            HideParam();
            HideProduct();
            HideChannel();
            HideKeyBCA();
        }

        function HideParam(){
            $("#inquiry-product").hide();
            $("#inquiry-channel").hide();
            $("#inquiry-keybca").hide();
        }

        function HideProduct() {
            $("#inquiry-product-atm").hide();
            $("#inquiry-product-creditcard").hide();
            $("#inquiry-product-account").hide();
            $("#inquiry-product-loan").hide();
        }

        function HideChannel() {
            $("#inquiry-channel-ib").hide();
            $("#inquiry-channel-connectib").hide();
            $("#inquiry-channel-requestib").hide();
            $("#inquiry-channel-mbca").hide();
            $("#inquiry-channel-smstopup").hide();
            $("#inquiry-channel-bbp").hide();
            $("#inquiry-channel-kbb").hide();
            $("#inquiry-channel-smsbca").hide();
        }

        function HideKeyBCA() {
            $("#inquiry-keybca-connectib").hide();
            $("#inquiry-keybca-requestib").hide();
            $("#inquiry-keybca-connectkbb").hide();
            $("#inquiry-keybca-requestkbb").hide();
        }
    });
</script>