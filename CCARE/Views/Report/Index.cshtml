        }

        function GetFilterValue() {
            var reportID = $("#Selected-ReportID").val();
            var postData = { _reportID: reportID };
            var url = '/Report/GetFiltersValue';
            $.ajax({
                type: "POST",
                url: url,
                data: postData,
                beforeSend: function () { },
                success: function (data) {
                    for (var i = 0 ; i < data.length ; i++) {
                        switch (data[i].key) {
                            case "Category":
                                $("#Selected-CategoryID").val(data[i].gid);
                                $("#Selected-Category").val(data[i].val);
                                break;
                            case "Product":
                                $("#Selected-ProductID").val(data[i].gid);
                                $("#Selected-Product").val(data[i].val);
                                break;
                            case "Workgroup":
                                $("#Selected-WorkgroupID").val(data[i].gid);
                                $("#Selected-Workgroup").val(data[i].val);
                                break;
                            case "Branch":
                                $("#Selected-BranchID").val(data[i].gid);
                                $("#Selected-Branch").val(data[i].val);
                                break;
                            case "SecurityRole":
                                $("#Selected-SecurityRoleID").val(data[i].gid);
                                $("#Selected-SecurityRole").val(data[i].val);
                                break;
                            case "BusinessUnit":
                                $("#Selected-BusinessUnitID").val(data[i].gid);
                                $("#Selected-BusinessUnit").val(data[i].val);
                                break;
                            case "Action":
                                $("#Selected-ActionID").val(data[i].gid);
                                $("#Selected-Action").val(data[i].val);
                                break;

                            case "StateCode":
                                $("#Selected-StateID").val(data[i].val);
                                $("#Selected-State").val(MappingSC(data[i].val, "statecode"));
                                break;
                            case "StatusCode":
                                $("#Selected-StatusID").val(data[i].val);
                                $("#Selected-Status").val(MappingSC(data[i].val, "statuscode"));
                                break;
                            case "IsDisabledStatusLbl":
                                $("#Selected-IsDisabledStatusID").val(data[i].val);
                                $("#Selected-IsDisabledStatus").val(MappingSC(data[i].val, "IsDisabledStatus"));
                                break;
                            case "ContactMethod":
                                $("#Selected-ContactMethodID").val(data[i].val);
                                $("#Selected-ContactMethod").val(MappingSC(data[i].val, "ContactMethod"));
                                break;

                            case "CreatedBy":
                                $("#Selected-UserID").val(data[i].gid);
                                $("#Selected-User").val(data[i].val);
                                break;
                            case "Owner":
                                $("#Selected-UserID").val(data[i].gid);
                                $("#Selected-User").val(data[i].val);
                                break;

                            case "PerformedByNames":
                                // specially for report user
                                $("#Selected-UserID").val(data[i].val);
                                $("#Selected-User").val(data[i].val);
                                break;
                            case "CustomerNo":
                                $("#CustNo").val(data[i].val);
                                break;
                            case "AccountNo":
                                $("#AcctNo").val(data[i].val);
                                break;
                            case "Request":
                                $("#RequestID").val(data[i].val);
                                break;
                        }
                    }
                },
                error: function (xhr, textStatus, errorThrown) { },
                complete: function () { }
            });
        }

        function MappingSC(input, mode) {
            var sc = input.split(',');
            var result = "";
            for (var i = 0; i < sc.length ; i++) {
                result = result == "" ? result : result + ",";
                if (mode == "statecode") {
                    result += sc[i] == 0 ? "Open"
                        : sc[i] == 1 ? "Completed"
                        : sc[i] == 2 ? "Canceled"
                        : "";
                } else if (mode == "statuscode") {
                    result += sc[i] == 0 ? "Open"
                        : sc[i] == 1 ? "New" : sc[i] == 2 ? "Assigned"
                        : sc[i] == 3 ? "Owned" : sc[i] == 4 ? "Incomplete"
                        : sc[i] == 5 ? "Closed" : sc[i] == 6 ? "Canceled"
                        : "";
                } else if (mode == "IsDisabledStatus") {
                    result += sc[i] == 0 ? "Active"
                        : sc[i] == 1 ? "Inactive"
                        : "";
                } else if (mode == "ContactMethod") {
                    result += sc[i] == 1 ? "Phone"
                        : sc[i] == 2 ? "Mail"
                        : sc[i] == 3 ? "Fax"
                        : sc[i] == 4 ? "Email"
                        : sc[i] == 5 ? "Web Chat"
                        : sc[i] == 6 ? "Media Cetak"
                        : sc[i] == 7 ? "Media Elektronik"
                        : sc[i] == 8 ? "Walk-in"
