    <script type="text/javascript">

        var ReportBU = "@ViewBag.ReportBU";
        var usr = "@Session["Fullname"].ToString()";

        ReportBU = ReportBU == "Business Unit/Branch Support" ? "BUS" : ReportBU;
        var uid = "UserIDs";

        $("#ReportParameter").hide();
        $("#ReportViewer").hide();
        var reportH = $(document).height();
        $("#Reports-Location").css("height", reportH - 150);

        function SetReportFilters() {
            var reportID = $("#Selected-ReportID").val();
            var postData = { _reportID: reportID };
            var url = '/Report/GetFilters';
            $.ajax({
                type: "POST",
                url: url,
                data: postData,
                beforeSend: function () { },
                success: function (data) {
                    $.each(data, function (i, value) {
                        $("#FilterType").append($('<option>').text(value.Text).attr('value', value.Value));
                        switch (value.Value) {
                            case "StartActualEnd":
                                $("#Actual").show();
                                if (!$("#ActualCB").is(":checked")) {
                                    $("#ActualCB").trigger("click");
                                    $("#ActualLbl").hide();
                                }
                                break;
                            case "EndActualEnd":
                                $("#Actual").show();
                                if (!$("#ActualCB").is(":checked")) {
                                    $("#ActualCB").trigger("click");
                                    $("#ActualLbl").hide();
                                }
                                break;

                            case "StartClosedOn":
                                $("#ClosedOn").show();
                                if (!$("#ClosedOnCB").is(":checked")) {
                                    $("#ClosedOnCB").trigger("click");
                                    $("#ClosedOnLbl").hide();
                                }
                                break;
                            case "EndClosedOn":
                                $("#ClosedOn").show();
                                if (!$("#ClosedOnCB").is(":checked")) {
                                    $("#ClosedOnCB").trigger("click");
                                    $("#ClosedOnLbl").hide();
                                }
                                break;

                            case "StartCreatedOn":
                                $("#CreatedOn").show();
                                if (!$("#CreatedOnCB").is(":checked")) {
                                    $("#CreatedOnCB").trigger("click");
                                    $("#CreatedOnLbl").hide();
                                }
                                break;
                            case "EndCreatedOn":
                                $("#CreatedOn").show();
                                if (!$("#CreatedOnCB").is(":checked")) {
                                    $("#CreatedOnCB").trigger("click");
                                    $("#CreatedOnLbl").hide();
                                }
                                break;

                            case "StartModifiedOn":
                                $("#ModifiedOn").show();
                                if (!$("#ModifiedOnCB").is(":checked")) {
                                    $("#ModifiedOnCB").trigger("click");
                                    $("#ModifiedOnLbl").hide();
                                }
                                break;
                            case "EndModifiedOn":
                                $("#ModifiedOn").show();
                                if (!$("#ModifiedOnCB").is(":checked")) {
                                    $("#ModifiedOnCB").trigger("click");
                                    $("#ModifiedOnLbl").hide();
                                }
                                break;

                            case "StartDate":
                                $("#Date").show();
                                if (!$("#DateCB").is(":checked")) {
                                    $("#DateCB").trigger("click");
                                    $("#DateLbl").hide();
                                }
                                break;
                            case "EndDate":
                                $("#Date").show();
                                if (!$("#DateCB").is(":checked")) {
                                    $("#DateCB").trigger("click");
                                    $("#DateLbl").hide();
                                }
                                break;

                            case "StartDueDate":
                                $("#DueDate").show();
                                if (!$("#DueDateCB").is(":checked")) {
                                    $("#DueDateCB").trigger("click");
                                    $("#DueDateLbl").hide();
                                }
                                break;
                            case "EndDueDate":
                                $("#DueDate").show();
                                if (!$("#DueDateCB").is(":checked")) {
                                    $("#DueDateCB").trigger("click");
                                    $("#DueDateLbl").hide();
                                }
                                break;

                            case "Category":
                                $("#CategoryZone").show(); $("#CategoryCB").trigger("click"); $("#CategoryLbl").hide();
                                break;
                            case "Product":
                                $("#ProductZone").show(); $("#ProductCB").trigger("click"); $("#ProductLbl").hide();
                                break;
                            case "Workgroup":
                                $("#WorkgroupZone").show(); $("#WorkgroupCB").trigger("click"); $("#WorkgroupLbl").hide();
                                break;
                            case "Branch":
                                $("#BranchZone").show(); $("#BranchCB").trigger("click"); $("#BranchLbl").hide();
                                break;
                            case "SecurityRole":
                                $("#SecurityRoleZone").show(); $("#SecurityRoleCB").trigger("click"); $("#SecurityRoleLbl").hide();
                                break;
                            case "BusinessUnit":
                                $("#BusinessUnitZone").show(); $("#BusinessUnitCB").trigger("click"); $("#BusinessUnitLbl").hide();
                                break;
                            case "Action":
                                $("#ActionZone").show(); $("#ActionCB").trigger("click"); $("#ActionLbl").hide();
                                break;

                            case "StateCode":
                                $("#StateZone").show(); $("#StateCB").trigger("click"); $("#StateLbl").hide();
                                break;
                            case "StatusCode":
                                $("#StatusZone").show(); $("#StatusCB").trigger("click"); $("#StatusLbl").hide();
                                break;
                            case "IsDisabledStatus":
                                $("#IsDisabledStatusZone").show(); $("#IsDisabledStatusCB").trigger("click"); $("#IsDisabledStatusLbl").hide();
                                break;
                            case "ContactMethod":
                                $("#ContactMethodZone").show(); $("#ContactMethodCB").trigger("click"); $("#ContactMethodLbl").hide();
                                break;

                            case "CreatedBy":
                                $("#UserZone").show(); $("#UserCB").trigger("click"); $("#UserLbl").hide();
                                $(".userlabel").html("Created By");
                                uid = "CreatedBys";
                                break;
                            case "Owner":
                                $("#UserZone").show(); $("#UserCB").trigger("click"); $("#UserLbl").hide();
                                $(".userlabel").html("Owner");
                                uid = "OwnerIDs";
                                break;
                            case "PerformedByNames":
                                $("#UserZone").show(); $("#UserCB").trigger("click"); $("#UserLbl").hide();
                                $(".userlabel").html("Performed By Names");
                                uid = "PerformedByNames";
                                break;

                            case "CustomerNo":
                                $("#CustomerNoZone").show(); $("#CustomerNoCB").trigger("click"); $("#CustomerNoLbl").hide();
                                break;

                            case "AccountNo":
                                $("#AccountNoZone").show(); $("#AccountNoCB").trigger("click"); $("#AccountNoLbl").hide();
                                break;

                            case "Request":
